@model IEnumerable<AssistMeProject.Models.User>



@{
    string title = "Questions";
    //Example of how to get data of user
    if (ViewBag.User != null)
    {
        title = ViewBag.User.USERNAME;
    }
    //End of example
    ViewData["Title"] = title;}


<section class="container main-content">
    <div class="row">
        <!--Espacios-->
        <div>
            <p> </p> <p> </p> <p> </p> <p> </p> <p> </p> <p> </p>
        </div>
        <div class="col-md-9">
            <div id="commentlist" class="page-content">
                <div class="boxedtitle page-title"><h2>Usuarios</h2></div>
                <ol class="commentlist clearfix">
                @foreach(var item in Model)
                {
                    <li class="user">
                        <div class="comment-body comment-body-answered clearfix">
                            <div class="avatar"><img alt="" src="~/images/women.png"></div>
                            <div class="comment-text">
                                <div class="author clearfix">
                                    <div class="comment-author"><a href="#">@Html.DisplayFor(modelItem => item.USERNAME)</a></div>

                                    <div class="text">
                                        <p> @Html.DisplayFor(modelItem => item.EMAIL)</p>
                                    </div>
                                    <a class="comment-reply" href="#"><i class="icon-reply"></i>Asignar rol de Administrador</a>
                                </div>
                            </div>
                        </div>

                    </li>
                    
                }
                    <li class="user">
                        <div class="comment-body comment-body-answered clearfix">
                            <div class="avatar"><img alt="" src="~/images/women.png"></div>
                            <div class="comment-text">
                                <div class="author clearfix">
                                    <div class="comment-author"><a href="#">Laura Hincapié</a></div>

                                    <div class="text">
                                        <p> Correo: lauhincapie97@globant.com</p>
                                    </div>
                                    <a class="comment-reply" href="#"><i class="icon-reply"></i>Asignar rol de Administrador</a>
                                </div>
                            </div>
                        </div>
                    </li>
                    <li class="user">
                        <div class="comment-body comment-body-answered clearfix">
                            <div class="avatar"><img alt="" src="~/images/men.png"></div>
                            <div class="comment-text">
                                <div class="author clearfix">
                                    <div class="comment-author"><a href="#">Camilo Jimenez</a></div>

                                    <div class="text">
                                        <p> Correo: camiloJilo@globant.com</p>
                                    </div>
                                    <a class="comment-reply" href="#"><i class="icon-reply"></i>Asignar rol de Administrador</a>
                                </div>
                            </div>
                        </div>
                    </li>
                    <li class="user">
                        <div class="comment-body comment-body-answered clearfix">
                            <div class="avatar"><img alt="" src="~/images/men.png"></div>
                            <div class="comment-text">
                                <div class="author clearfix">
                                    <div class="comment-author"><a href="#">Julian Mabesoy</a></div>

                                    <div class="text">
                                        <p> Correo: julianmabe@globant.com</p>
                                    </div>
                                    <a class="comment-reply" href="#"><i class="icon-reply"></i>Asignar rol de Administrador</a>
                                </div>
                            </div>
                        </div>
                    </li>
                    <li class="user">
                        <div class="comment-body comment-body-answered clearfix">
                            <div class="avatar"><img alt="" src="~/images/men.png"></div>
                            <div class="comment-text">
                                <div class="author clearfix">
                                    <div class="comment-author"><a href="#">Juan David Lopez</a></div>

                                    <div class="text">
                                        <p> Correo: juan123@globant.com</p>
                                    </div>
                                    <a class="comment-reply" href="#"><i class="icon-reply"></i>Asignar rol de Administrador</a>
                                </div>
                            </div>
                        </div>
                    </li>


                </ol><!-- End commentlist -->
            </div><!-- End page-content -->



            <div class="post-next-prev clearfix">
                <p class="prev-post">
                    <a href="#"><i class="icon-double-angle-left"></i>&nbsp;Prev question</a>
                </p>
                <p class="next-post">
                    <a href="#">Next question&nbsp;<i class="icon-double-angle-right"></i></a>
                </p>
            </div><!-- End post-next-prev -->
        </div><!-- End main -->
        <aside class="col-md-3 sidebar">
            <div class="widget widget_stats">
                <h3 class="widget_title">Stats</h3>
                <div class="ul_list ul_list-icon-ok">
                    <ul>
                        <li><i class="icon-question-sign"></i>Questions ( <span>20</span> )</li>
                        <li><i class="icon-comment"></i>Answers ( <span>50</span> )</li>
                    </ul>
